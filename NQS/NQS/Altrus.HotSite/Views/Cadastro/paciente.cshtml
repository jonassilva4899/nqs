                            <a href="#" class="ms-text-primary">Patient List</a>
                        </div>
                        <div class="ms-panel-body">
                            <form class="needs-validation" novalidate>
                                <div class="form-row">
                                    <div class="col-md-6 mb-3">
                                        <label for="validationCustom010">Documento de Identificação</label>
                                        <div class="input-group">
                                            @*<input type="text" class="form-control" id="validationCustom010" placeholder="Enter First Name" required>*@                                            
                                            <input class="form-control" type="text" asp-for="DocumentoIdentificacao" maxlength="150" placeholder="Informe o documento de identificação">
                                            <span asp-validation-for="DocumentoIdentificacao" class="text-danger"></span>
                                        </div>
                                    </div>

                                    <div class="col-md-6 mb-3">
                                        <label for="validationCustom020">Nome Completo:</label>
                                        <div class="input-group">
                                            <input type="text" class="form-control" asp-for="NomeCompleto" placeholder="Informe o nome do paciente">
                                            <span asp-validation-for="NomeCompleto" class="text-danger"></span>
                                            <div class="valid-feedback">
                                                Looks good!
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <div class="form-row">
                                    <div class="col-md-6 mb-2">
                                        <label for="validationCustom040">Create Id</label>
                                        <div class="input-group">
                                            <input type="text" class="form-control" id="validationCustom040" placeholder="Id" required>

                                        </div>
                                    </div>

                                    <div class="col-md-6 mb-2">
                                        <label for="validationCustom030">Mobile No.</label>
                                        <div class="input-group">
                                            <input type="text" class="form-control" id="validationCustom030" placeholder="Mo" required>

                                        </div>
                                    </div>
                                </div>


                                <div class="form-row">
                                    <div class="col-md-6 mb-3">
                                        <label for="validationCustom050">Email</label>
                                        <div class="input-group">
                                            <input type="email" class="form-control" id="validationCustom050" placeholder="Add Email Id" required>

                                        </div>
                                    </div>
                                    <div class="col-md-6 mb-3">
                                        <label>Blood Group</label>
                                        <div class="input-group">
                                            <select class="form-control" id="validationCustom220" required>
                                                <option value="">A+</option>
                                                <option value="">A-</option>
                                                <option value="">B+</option>
                                                <option value="">B-</option>
                                                <option value="">AB+</option>
                                                <option value="">AB-</option>
                                                <option value="">O+</option>
                                                <option value="">O-</option>

                                            </select>

                                        </div>
                                    </div>
                                </div>

                                <div class="form-row">
                                    <div class="col-md-6 mb-3">
                                        <label>Occupation</label>
                                        <div class="input-group">
                                            <input type="text" class="form-control" id="validationCustom180" placeholder="Enter Occupation" required>

                                        </div>
                                    </div>

                                    <div class="col-md-6 mb-3">
                                        <label>Marital status</label>
                                        <div class="input-group">
                                            <select class="form-control" id="validationCustom230" required>
                                                <option value="">Married</option>
                                                <option value="">Unmarried</option>
                                            </select>
                                            <div class="invalid-feedback">
                                                Please select a Country.
                                            </div>
                                        </div>
                                    </div>
                                </div>



                                <div class="form-row">
                                    <div class="col-md-6 mb-3">
                                        <label>Patient Image</label>
